{
  "name": "@nightowl-developers/generator-nightowl-react",
  "version": "0.1.3",
  "description": "The nightowl developers yoeman generator for a react application that uses babel and webpack.",
  "scripts": {
    "publish:alpha": "npm publish --access=public --scope=@nightowl-developers --tag=alpha",
    "publish:beta": "npm publish --access=public --scope=@nightowl-developers --tag=beta",
    "publish": "npm publish --access=public --scope=@nightowl-developers --tag=latest",
    "test": "nyc --reporter=html ./node_modules/mocha/bin/mocha"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator",

    "component",
    "design",
    "css",
    "sass",
    "scss",
    "less",
    "styled-components",
    "eslint",
    "airbnb",
    "prettier",
    "webpack",
    "eslint",
    "development",
    "production",
    "optimized"
  ],
  "dependencies": {
    "yeoman-generator": "^4.5.0"
  },
  "author": "Joshua Blevins <josh.adam.blevins@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "fs-extra": "^9.0.1",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "path": "^0.12.7",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.6.0"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
